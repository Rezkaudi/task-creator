{
  "name": "task-creator",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "install": "npm install --prefix frontend && npm install --prefix backend"
  },
  "dependencies": {},
  "devDependencies": {
    "concurrently": "^8.0.1"
  }
}
